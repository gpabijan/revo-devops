{
  "type": "postgres",
  "host": "`${env.DB2_HOST}`",
  "port": "5432",
  "username": "process.env.DB2_USERNAME",
  "password": "process.env.DB2_PASSWORD",
  "database": "process.env.DB2_NAME",
  "entities": [
    "process.env.DB2_ENTITIES"
  ],
  "synchronize": true,
  "cli": {
    "entitiesDir": "process.env.DB2_ENTITIESDIER"
  }
}



